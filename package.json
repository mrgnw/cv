{
    "name": "morgan-cv",
    "version": "0.0.2",
    "private": true,
    "scripts": {
        "dev": "vite dev --host",
        "build": "vite build",
        "preview": "vite preview",
        "prev": "vite build && vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "install-browsers": "playwright install",
        "pdf": "SERVER_URL=https://cv.skate-in.ts.net node pdf-cli.js",
        "pdf:watch": "SERVER_URL=https://cv.skate-in.ts.net node pdf-cli.js --watch",
        "pdf:force": "SERVER_URL=https://cv.skate-in.ts.net node pdf-cli.js --force all",
        "pdf:list": "SERVER_URL=https://cv.skate-in.ts.net node pdf-cli.js --list",
        "pdf:changed": "SERVER_URL=https://cv.skate-in.ts.net node pdf-cli.js --changed",
        "test": "vitest",
        "test:run": "vitest run",
        "test:experience-rendering": "vitest run tests/experience-rendering.test.js",
        "test:route-integration": "vitest run tests/route-integration.test.js",
        "test:unit": "vitest run tests/experience-rendering.test.js tests/route-integration.test.js",
        "test:content-optimization": "node test-content-optimization-fix.mjs",
        "test:experience": "node test-experience.mjs",
        "test:integration": "node test-version-integration.mjs",
        "test:validate": "node validate-experience.mjs",
        "test:optimization": "node test-content-optimization.mjs",
        "test:all": "node run-all-tests.mjs",
        "pdf:all": "SERVER_URL=https://cv.skate-in.ts.net node pdf-cli.js all",
        "pdf:all:force": "SERVER_URL=https://cv.skate-in.ts.net node pdf-cli.js all --force"
    },
    "devDependencies": {
        "@sveltejs/adapter-cloudflare": "^4.7.2",
        "@sveltejs/adapter-node": "^5.3.1",
        "@sveltejs/kit": "^2.5.27",
        "@sveltejs/vite-plugin-svelte": "^4.0.0",
        "autoprefixer": "^10.4.20",
        "chokidar": "^3.5.3",
        "jsdom": "^27.0.1",
        "mdsvex": "^0.11.2",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "svelte-image": "^0.2.9",
        "tailwindcss": "^3.4.9",
        "typescript": "^5.5.0",
        "vite": "^5.4.4",
        "vitest": "^1.6.1"
    },
    "type": "module",
    "dependencies": {
        "@cloudflare/workerd-darwin-arm64": "^1.20251024.0",
        "@iconify/svelte": "^4.0.2",
        "@sveltejs/adapter-static": "^3.0.5",
        "bits-ui": "^2.9.6",
        "caniuse-lite": "^1.0.30001741",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "json5": "^2.2.3",
        "lucide-svelte": "^0.452.0",
        "pdfmake": "^0.2.13",
        "playwright": "^1.48.0",
        "svelte-typewriter": "^3.2.3",
        "tailwind-merge": "^2.5.2",
        "tailwind-variants": "^0.2.1"
    }
}
